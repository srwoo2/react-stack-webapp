{
  "name": "react-stack-webapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config ./webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "start": "npm run build && cross-env NODE_ENV=production node ./server/server.js",
    "server:dev": "cross-env NODE_ENV=development USE_DB=true node ./server/server.js",
    "server:no-db": "cross-env NODE_ENV=development USE_DB=false node ./server/server.js",
    "db:setup": "node ./server/config/db.js",
    "deploy": "vercel --prod",
    "test": "jest",
    "lint": "eslint --cache ./",
    "lint:fix": "eslint ./ --fix",
    "format": "prettier --cache --write .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "docs": "jsdoc -c jsdoc.config.json ."
  },
  "engines": {
    "node": "20.10.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@sentry/react": "^10.32.1",
    "@tanstack/react-virtual": "^3.13.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "grommet": "^2.19.1",
    "grommet-icons": "^4.7.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "uuid": "^3.4.0",
    "web-vitals": "^1.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.9.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.1.7",
    "iconv-lite": "^0.6.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "lint-staged": "^15.5.0",
    "mini-css-extract-plugin": "^2.7.7",
    "node-mocks-http": "^1.14.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "sass": "^1.86.1",
    "sass-loader": "^16.0.5",
    "style-loader": "^3.3.4",
    "styled-components": "^6.1.8",
    "supertest": "^6.3.3",
    "typescript": "^4.9.5",
    "vercel": "^37.8.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-dev-server": "^4.15.1",
    "webpack-hot-middleware": "^2.26.1"
  }
}
